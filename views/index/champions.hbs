<div class="row row-cols-1 row-cols-md-4 g-4">
    {{#each nameAndImg}}
    <div class="col">
        <div class="card">
            <img src="{{img}}" class="card-img-top" alt="...">
            <h5 class="card-title text-center"></h5>
            <div class="card-body">
                <a href="/champion-details/{{name}}" class="btn btn-primary">{{name}}</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>